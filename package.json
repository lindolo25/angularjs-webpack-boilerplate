{
	"name": "angularjs-webpack-boilerplate",
	"version": "0.0.0-development",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "npx webpack --config webpack.config.js --env.production",
		"dev": "webpack-dev-server --env.dev"
	},
	"author": "",
	"license": "ISC",
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/github",
				{
					"assets": [
						{
							"path": "*.zip",
							"label": "Distribution"
						}
					]
				}
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"autoprefixer": "^9.7.3",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.3.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^6.7.3",
		"eslint": "^6.7.2",
		"eslint-loader": "^4.0.2",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.0",
		"node-forge": ">=0.10.0",
		"node-sass": "^4.13.1",
		"postcss-loader": "^7.0.2",
		"precss": "^4.0.0",
		"sass-loader": "^13.2.0",
		"semantic-release": "^17.3.0",
		"style-loader": "^1.3.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"angular": "^1.8.0",
		"angular-ui-router": "^1.0.22",
		"bootstrap": "^4.4.1",
		"jquery": "^3.5.0",
		"oclazyload": "^1.1.0",
		"popper.js": "^1.16.0"
	}
}